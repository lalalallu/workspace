.pageview{
    width: 100%;
    height: 100vh;
    display: flex;
    flex-direction: column;
    background-color: rgb(221, 251, 241);
    /* border: 1px solid #000; */
}
.topview{
    width: 100%;
    height: 15%;
    display: flex;
    border: 1px solid #000;
    justify-content: center;
    align-items: center;
}
.inview{
    width: 70%;
    height: 60%;
    border: 1px solid #000;
    border-radius: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.sarchview{
    width: 12%;
    height: 50%;
    /* border: 1px solid #000; */
    margin-left: -40px;
    z-index: 999;
}
.cimg{
    width: 100%;
    height: 100%;
}
.scview{
    width: 100%;
    height: 85%;
    /* display: flex; */
    border: 1px solid #000;
}
.contentview{
    width: 100%;
    height: 100%;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
}
.item{
    width: 40%;
    height: 35%;
    margin: 10px;
    display: flex;
    border: 1px solid #000;
    flex-direction: column;
}
.nameview{
    width: 100%;
    height: 30%;
    border: 1px solid #000;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 15px;
    font-weight: bold;
    font-family: 楷体;
}
.cityview{
    width: 100%;
    height: 20%;
    border: 1px solid #000;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 15px;
    font-weight: bold;
    font-family: 楷体;
}
.levelview{
    width: 100%;
    height: 20%;
    border: 1px solid #000;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 15px;
    font-weight: bold;
    font-family: 楷体;
}
.webview{
    width: 100%;
    height: 30%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    font-size: 10px;
    border: 1px solid #000;
}